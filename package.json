{
  "name": "componentjs-generator",
  "version": "1.0.0",
  "description": "A tool to generate and fix .jsonld component files for Components.js TypeScript classes",
  "main": "bin/GenerateRunner.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest ${1}",
    "prepare": "npm run build"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "collectCoverage": true
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bin": {
    "componentsjs-generate": "bin/GenerateRunner.js",
    "componentsjs-fix": "bin/FixRunner.js",
    "componentsjs-fixpackage": "bin/FixPackageRunner.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.json",
    "index.d.ts",
    "index.ts"
  ],
  "dependencies": {
    "@comunica/core": "latest",
    "child_process": "^1.0.2",
    "comment-parser": "^0.6.1",
    "componentsjs": "^3.2.1",
    "jsonld": "^1.6.2",
    "jsonld-context-parser": "^1.3.0",
    "minimist": "^1.2.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1",
    "@typescript-eslint/typescript-estree": "^1.13.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.18",
    "@types/jsonld": "^1.5.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.6.9",
    "@types/rimraf": "^2.0.2",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "rimraf": "latest",
    "ts-jest": "^24.0.2"
  }
}
